<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dip" >

            <ImageButton
                android:id="@+id/imgasset"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:onClick="onButtonClicked"
                android:src="@drawable/myland2" />

            <ImageButton
                android:id="@+id/imgetc"
                android:layout_width="90dip"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/padding_large"
                android:layout_marginRight="@dimen/padding_large"
                android:onClick="onButtonClicked"
                android:src="@drawable/myland" />

            <ImageButton
                android:id="@+id/imgdebit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/padding_small"
                android:layout_marginLeft="@dimen/padding_large"
                android:onClick="onButtonClicked"
                android:src="@drawable/buche" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1dp" >

            <TextView
                android:id="@+id/tvuserasset"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="원"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/tvuseretc"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="원"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/tvuserdebit"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="원"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="1dp" >

            <TextView
                android:id="@+id/textView4"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:gravity="center"
                android:text="금융 자산"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="기타 자산"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:gravity="center"
                android:text="부채"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dip" >

            <ImageButton
                android:id="@+id/imgjicul"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:onClick="onButtonClicked"
                android:src="@drawable/jicul" />

            <ImageButton
                android:id="@+id/imguserprivate"
                android:layout_width="90dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_large"
                android:layout_marginRight="@dimen/padding_large"
                android:onClick="onButtonClicked"
                android:src="@drawable/userinfor" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/tvuserjicul"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="원"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/tvuserprivate"
                android:layout_width="100dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="개인 정보"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView9"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="지출입"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
